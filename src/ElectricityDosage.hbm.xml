<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.xmu.nextgencomm.model.ElectricityDosage"
		lazy="false" table="electricityDosage">
		<composite-id>
			<key-property name="serialNum" type="string"></key-property>
			<key-property name="date" type="java.sql.Date"></key-property>
		</composite-id>
		<property name="dosage" type="double" not-null="true" column="dosage" />
		<many-to-one name="house" cascade="all"
			class="cn.edu.xmu.nextgencomm.model.House" column="house_id"></many-to-one>
	</class>
</hibernate-mapping>